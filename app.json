{
  "expo": {
    "name": "DRUSHTI",
    "slug": "eye",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/eyeand.png",
    "scheme": "eye",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "foregroundImage": "./assets/images/eyeios.png",
      "backgroundColor": "#3a8dde",
      "bundleIdentifier": "com.meghana2002.eye",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/eyeand.png",
        "backgroundColor": "#3a8dde"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.meghana2002.eye"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/eyeweb.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b3cd9ab1-0fcf-4773-8d2f-101e82c78a7c"
      }
    }
  }
}
